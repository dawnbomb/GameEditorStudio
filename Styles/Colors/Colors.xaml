    <SolidColorBrush x:Key="LightTitleBarForeground" Color="Black"/>

    <SolidColorBrush x:Key="DarkTitleBarBackground" Color="Black"/>
    <SolidColorBrush x:Key="DarkTitleBarForeground" Color="Black"/>
    

    <!--   Other Colors   -->
    <SolidColorBrush x:Key="White" Color="White"/>
    <SolidColorBrush x:Key="Black" Color="Black"/>
    <SolidColorBrush x:Key="Gray" Color="#888888"/>

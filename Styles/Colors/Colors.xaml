    <SolidColorBrush x:Key="DarkMode_TreeSelectedForeGround"          Color="#FFFFFF"/>
    <SolidColorBrush x:Key="DarkMode_TreeSelectedBackGround"          Color="#444444"/>
    
    <!--   Delete These Later  -->
    <SolidColorBrush x:Key="LightForeground" Color="#000000" />

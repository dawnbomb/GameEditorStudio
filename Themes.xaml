<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Here, UserSettings.xaml.cs, LibraryMan-->
    
    <SolidColorBrush x:Key="ApplicationText" Color="#CFCBC7"/> 
    <SolidColorBrush x:Key="ApplicationBack" Color="#1F1F1F"/>
    <SolidColorBrush x:Key="ApplicationBorder" Color="#121212"/>
    <!--No App Border-->
    <SolidColorBrush x:Key="HeaderText" Color="#800000"/>
    <SolidColorBrush x:Key="HeaderBack" Color="#4E4E4E"/>
    <SolidColorBrush x:Key="HeaderBorder" Color="#800000"/>

    <SolidColorBrush x:Key="PanelText" Color="#800000"/>
    <SolidColorBrush x:Key="PanelBack" Color="#333333"/>
    <SolidColorBrush x:Key="PanelBorder" Color="#FF000000"/>

    <SolidColorBrush x:Key="TextboxText" Color="#CFCBC7"/> 
    <SolidColorBrush x:Key="TextboxBack" Color="#18191B"/> 
    <SolidColorBrush x:Key="TextboxBorder" Color="#0A0A0A"/> 
    <SolidColorBrush x:Key="TextboxHighlightText" Color="#FF000000"/> <!--Actually the text back highlight for some reason-->
    <SolidColorBrush x:Key="TextboxHighlightBack" Color="#FF000000"/>  <!--Unused-->
    <!--No Highlight Border-->

    <SolidColorBrush x:Key="MenuText" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MenuBack" Color="#1a1a1a"/>
    <SolidColorBrush x:Key="MenuBorder" Color="#1a1a1a"/>
    <SolidColorBrush x:Key="MenuMouseoverText" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MenuMouseoverBack" Color="#333333"/>
    <SolidColorBrush x:Key="MenuMouseoverBorder" Color="#959595"/>

    <SolidColorBrush x:Key="ButtonText" Color="#FFE1E1E1"/>  
    <SolidColorBrush x:Key="ButtonBack" Color="#FF232323"/> 
    <SolidColorBrush x:Key="ButtonBorder" Color="#FF5D5D5D"/> 
    <SolidColorBrush x:Key="ButtonMouseoverText" Color="#FFE1E1E1"/> 
    <SolidColorBrush x:Key="ButtonMouseoverBack" Color="#FF313131"/>
    <SolidColorBrush x:Key="ButtonMouseoverBorder" Color="#FFFFFF"/> 
    <SolidColorBrush x:Key="ButtonDownText" Color="#FFE1E1E1"/> 
    <SolidColorBrush x:Key="ButtonDownBack" Color="#FF5D5D5D"/> 
    <SolidColorBrush x:Key="ButtonDownBorder" Color="#FF5D5D5D"/>

    <SolidColorBrush x:Key="CheckboxText" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="CheckboxBack" Color="#272729"/>
    <SolidColorBrush x:Key="CheckboxBorder" Color="#FF0E0E0E"/>
    

    <SolidColorBrush x:Key="ListText" Color="#FFFFFF"/> 
    <SolidColorBrush x:Key="ListBack" Color="#1F1F1F"/>   
    <SolidColorBrush x:Key="ListBorder" Color="#000000"/>  
    <SolidColorBrush x:Key="ListMouseoverText" Color="#FFFFFF"/> 
    <SolidColorBrush x:Key="ListMouseoverBack" Color="#2E2E2E"/> 
    <SolidColorBrush x:Key="ListMouseoverBorder" Color="#808080"/>  
    <SolidColorBrush x:Key="ListSelectedText" Color="#FFFFFF"/> 
    <SolidColorBrush x:Key="ListSelectedBack" Color="#444444"/> 
    <SolidColorBrush x:Key="ListSelectedBorder" Color="#808080"/> 

    <SolidColorBrush x:Key="DropDownText" Color="#FFFFFFFF"/> 
    <SolidColorBrush x:Key="DropDownBack" Color="#1A1B1C"/> 
    <SolidColorBrush x:Key="DropDownBorder" Color="#0A0A0A"/> 
    <SolidColorBrush x:Key="DropDownItemMouseoverText" Color="#FFFFFFFF"/> 
    <SolidColorBrush x:Key="DropDownItemMouseoverBack" Color="#FF212325"/> 
    <SolidColorBrush x:Key="DropDownItemMouseoverBorder" Color="#FF8F8F8F"/>

    <SolidColorBrush x:Key="EntryBack" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="EntryBorder" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="SelectedEntryBack" Color="#1A1B1C"/>
    <SolidColorBrush x:Key="SelectedEntryBorder" Color="#1A1B1C"/>

    <SolidColorBrush x:Key="HiddenEntryBack" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="HiddenEntryBorder" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="HiddenSelectedEntryBack" Color="#1A1B1C"/>
    <SolidColorBrush x:Key="HiddenSelectedEntryBorder" Color="#1A1B1C"/>

</ResourceDictionary>